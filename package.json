{
  "name": "product-feedback-api",
  "version": "1.0.0",
  "description": "backend API for product feedback",
  "main": "index.js",
  "scripts": {
    "build": "npm i && npx prisma generate --schema=./src/prisma/schema.prisma && npx prisma migrate dev --name init --schema ./src/prisma/schema.prisma && npx tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "preserve": "npm run build",
    "serve": "concurrently \"npx tsc -w\" \" nodemon dist/index.js\""
  },
  "author": "arunpariyar",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "install": "^0.13.0",
    "morgan": "^1.10.0",
    "prisma": "^5.10.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
